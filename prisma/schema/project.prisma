model Project {
  id               Int      @id @default(autoincrement())
  name             String
  picture          Media    @relation(fields: [pictureId], references: [id])
  pictureId        Int
  createdAt        DateTime @default(now())
  shortDescription String   @db.VarChar(200)
  description      String?  @db.Text
  url              String?
  repoUrl          String?
  technosUsed      Techno[]
  isPublished      Boolean
}
